<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<esper-configuration
	xmlns="http://www.espertech.com/schema/esper"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="esper-configuration-8-0.xsd">
	<common>
		<event-type name="Prueba2" type="Output">
			<objectarray>
				<objectarray-property class="float" name="Sum" />
			</objectarray>
		</event-type>
		<event-type name="AirMeasurement" type="Input">
			<objectarray>
				<objectarray-property class="long"
					name="stationTs" />
				<objectarray-property class="int"
					name="stationId" />
				<objectarray-property class="double"
					name="pm2_5" />
				<objectarray-property class="double"
					name="pm10" />
				<objectarray-property class="double" name="o3" />
				<objectarray-property class="double" name="no2" />
				<objectarray-property class="double" name="so2" />
				<objectarray-property class="double" name="co" />
			</objectarray>
		</event-type>
		<event-type name="NO2_Avg" type="Output">
			<objectarray>
				<objectarray-property class="long"
					name="timestamp" />
				<objectarray-property class="int"
					name="stationId" />
				<objectarray-property class="double"
					name="value" />
			</objectarray>
		</event-type>
		<loggin>
			<jdbc enabled="false" />
			<query-plan enabled="false" />
		</loggin>
	</common>
	<compiler>
		<view-resources>
			<allow-multiple-expiry-policy
				enabled="true" />
			<stream-selection>
				<stream-selector value="istream" />
			</stream-selection>
		</view-resources>
	</compiler>
	<runtime />
</esper-configuration>