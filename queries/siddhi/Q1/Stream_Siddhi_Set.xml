<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<siddhi-configuration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<common>
		<event-type name="AirMeasurement" type="Input">
			<objectarray>
				<objectarray-property class="int"
					name="stationId" />
				<objectarray-property class="double" name="no2" />
				<objectarray-property class="long"
					name="stationTs" />
			</objectarray>
		</event-type>
				<event-type name="P2" type="Input">
			<objectarray>
				<objectarray-property class="int" name="statio" />
				<objectarray-property class="double" name="so2" />
				<objectarray-property class="long" name="tsp" />
			</objectarray>
		</event-type>
		<event-type name="NO2_Avg_s" type="Output">
			<objectarray>
				<objectarray-property class="int"
					name="stationId" />
				<objectarray-property class="double"
					name="value" />
				<objectarray-property class="long" name="TS" />
			</objectarray>
		</event-type>
		<event-type name="NO2_Avg_s2" type="Output">
			<objectarray>
				<objectarray-property class="int"
					name="stationId2" />
				<objectarray-property class="double"
					name="value2" />
				<objectarray-property class="long" name="TS2" />
			</objectarray>
		</event-type>
	</common>
</siddhi-configuration>