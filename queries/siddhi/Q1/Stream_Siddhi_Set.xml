<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<siddhi-configuration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<common>
		<event-type name="otro" type="Input">
			<objectarray>
				<objectarray-property class="bool" name="as" />
				<objectarray-property class="double"
					name="asfgt" />
			</objectarray>
		</event-type>
		<event-type name="Nuevo" type="Input">
			<objectarray>
				<objectarray-property class="bool" name="bul" />
				<objectarray-property class="int" name="entero" />
				<objectarray-property class="long" name="TeS" />
			</objectarray>
		</event-type>
		<event-type name="Prueba" type="Output">
			<objectarray>
				<objectarray-property class="int" name="entero" />
				<objectarray-property class="long" name="TeS" />
			</objectarray>
		</event-type>
		<event-type name="AirMeasurement" type="Input">
			<objectarray>
				<objectarray-property class="long"
					name="stationTs" />
				<objectarray-property class="int"
					name="stationId" />
				<objectarray-property class="double"
					name="pm2_5" />
				<objectarray-property class="double"
					name="pm10" />
				<objectarray-property class="double" name="o3" />
				<objectarray-property class="double" name="no2" />
				<objectarray-property class="double" name="so2" />
				<objectarray-property class="double" name="co" />
			</objectarray>
		</event-type>
	</common>
</siddhi-configuration>